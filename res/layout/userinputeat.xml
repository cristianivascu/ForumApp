<?xml version="1.0" encoding="utf-8"?>


<LinearLayout 
xmlns:android="http://schemas.android.com/apk/res/android"
android:orientation="vertical" 
android:layout_width="fill_parent" 
android:layout_height="fill_parent"
android:id="@+id/prompteat">

<EditText
android:id="@+id/inputeat1"
android:layout_width="fill_parent" 
android:layout_height="wrap_content" 
android:hint="Enter Name"/>
<EditText
android:id="@+id/inputeat2"
android:layout_width="fill_parent" 
android:layout_height="wrap_content" 
android:hint="Enter Distance in Minutes"/>
<EditText
android:id="@+id/inputeat3"
android:layout_width="fill_parent" 
android:layout_height="wrap_content" 
android:hint="Enter Cuisine" />
<EditText
android:layout_width="fill_parent" 
android:id="@+id/inputeat4"
android:layout_height="wrap_content" 
android:hint="Enter Address"/> 
 <EditText
android:id="@+id/inputeat5"
android:layout_width="fill_parent" 
android:layout_height="wrap_content" 
android:hint="Enter Phone Number"/>

<EditText
android:id="@+id/inputeat6"
android:layout_width="fill_parent" 
android:layout_height="wrap_content" 
android:hint="Enter Website"/>

</LinearLayout>

